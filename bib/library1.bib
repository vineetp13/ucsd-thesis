issn = {1091-6490},
journal = {Proceedings of the National Academy of Sciences of the United States of America},
number = {19},
pages = {7716--7720},
pmid = {21518867},
title = {{Role of test motivation in intelligence testing}},
volume = {108},
year = {2011}
}
