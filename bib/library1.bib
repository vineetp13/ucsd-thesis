@article{Kirshner2004,
abstract = {Drawing on examples from science and medical education, evidence and explanations for $\backslash$r$\backslash$nthe superiority of guided instruction are explained in the context of expert-novice $\backslash$r$\backslash$ndifferences, cognitive load and cognitive architecture. While unguided or minimally$\backslash$r$\backslash$nguided learning approaches are very popular and intuitively appealing, the point is made $\backslash$r$\backslash$nthat evidence from empirical studies over the past half century consistently indicates that $\backslash$r$\backslash$nthey are less effective and less efficient than learning approaches that place a strong effort $\backslash$r$\backslash$non guidance of the student learning process. Only when learners have sufficient high $\backslash$r$\backslash$nprior knowledge that provides ‘internal' guidance does the advantage of guidance begin $\backslash$r$\backslash$nto reduce. Recent developments in instructional research and instructional design models $\backslash$r$\backslash$nthat support guided learning are briefly described.},
