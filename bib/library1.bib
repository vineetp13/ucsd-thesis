journal = {Proceedings of the 33rd Annual ACM Conference on Human Factors in Computing Systems - CHI '15},
keywords = {Crowdsourcing,Freemium,Gamification,Psychophysics},
pages = {1065--1074},
title = {{Mobile Gamification for Crowdsourcing Data Collection}},
