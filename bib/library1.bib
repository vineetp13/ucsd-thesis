@article{Marsh1996,
abstract = {Three experiments were performed to test Smith, Ward, and Schumacher's (1993) conformity hy-pothesis-that people's ideas will conform to examples they are shown in a creative generation task. Conformity was observed in all three experiments; participants tended to incorporate critical features of experimenter-provided examples. However, examination of total output, elaborateness of design, and the noncritical features did not confirm that the conformity effect constrained creative output in any of the three experiments. Increasing the number of examples increased the conformity effect (Ex-periment 1). Examples that covaried features that are naturally uncorrelated in the real world led to a greater subjective rating of creativity (Experiment 2). A delay between presentation and test increased conformity (Experiment 3), just as models of inadvertent plagiarism would predict. The explanatory power of theoretical accounts such as activation, retrieval blocking, structured imagination, and cate-gory abstraction are evaluated.},
author = {Marsh, Richard L and Landau, Joshua D and Hicks, Jason L},
doi = {10.3758/BF03201091},
file = {:Users/vineetp/Library/Application Support/Mendeley Desktop/Downloaded/Marsh, Landau, Hicks - 1996 - How examples may (and may not) constrain creativity(2).pdf:pdf},
isbn = {0090-502X},
issn = {0090-502X},
journal = {Memory {\&} Cognition},
number = {5},
pages = {669--680},
pmid = {8870535},
title = {{How examples may (and may not) constrain creativity}},
volume = {24},
year = {1996}
}
@article{Abdurachmanov2015,
abstract = {Power consumption will be a key constraint on the future growth of Distributed High Throughput Computing (DHTC) as used by High Energy Physics (HEP). This makes performance-per-watt a crucial metric for selecting cost-efficient computing solutions. For this paper, we have done a wide survey of current and emerging architectures becoming available on the market including x86-64 variants, ARMv7 32-bit, ARMv8 64-bit, Many-Core and GPU solutions, as well as newer System-on-Chip (SoC) solutions. We compare performance and energy efficiency using an evolving set of standardized HEP-related benchmarks and power measurement techniques we have been developing. We evaluate the potential for use of such computing solutions in the context of DHTC systems, such as the Worldwide LHC Computing Grid (WLCG).},
archivePrefix = {arXiv},
arxivId = {1510.03676},
author = {Abdurachmanov, David and Elmer, Peter and Eulisse, Giulio and Knight, Robert},
