@article{Corrigan-Gibbs2015,
abstract = {Many Internet services depend on the integrity of their users, even when$\backslash$nthese users have strong incentives to behave dishonestly. Drawing on$\backslash$nexperiments in two different online contexts, this study measures the$\backslash$nprevalence of cheating and evaluates two different methods for deterring$\backslash$nit. Our first experiment investigates cheating behavior in a pair of$\backslash$nonline exams spanning 632 students in India. Our second experiment$\backslash$nexamines dishonest behavior on Mechanical Turk through an online task$\backslash$nwith 2,378 total participants. Using direct measurements that are not$\backslash$ndependent on self-reports, we detect significant rates of cheating in$\backslash$nboth environments. We confirm that honor codes despite frequent use in$\backslash$nmassive open online courses (MOOCs) lead to only a small and$\backslash$ninsignificant reduction in online cheating behaviors. To overcome these$\backslash$nchallenges, we propose a new intervention: a stern warning that spells$\backslash$nout the potential consequences of cheating. We show that the warning$\backslash$nleads to a significant (about twofold) reduction in cheating, consistent$\backslash$nacross experiments. We also characterize the demographic correlates of$\backslash$ncheating on Mechanical Turk. Our findings advance the understanding of$\backslash$ncheating in online environments, and suggest that replacing traditional$\backslash$nhonor codes with warnings could be a simple and effective way to deter$\backslash$ncheating in online courses and online labor marketplaces.},
author = {Corrigan-Gibbs, Henry and Gupta, Nakull and Northcutt, Curtis and Cutrell, Edward and Thies, William},
doi = {10.1145/2810239},
