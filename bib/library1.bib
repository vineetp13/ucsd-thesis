isbn = {9781450343350},
keywords = {Author Keywords Extreme Citizen Science,HCI),Health,Miscellaneous,Nutrition Literacy,Self-experimentation,Self-tracking},
pages = {2112--2123},
title = {{Soylent Diet Self-Experimentation: Design Challenges in Extreme Citizen Science Projects}},
year = {2017}
}
