keywords = {assessment,crowdsourcing,learning,review},
pages = {1445--1455},
title = {{Reviewing versus Doing: Learning and Performance in Crowd Assessment}},
url = {http://dl.acm.org/citation.cfm?id=2531602.2531718},
year = {2014}
}
