pages = {1393--1402},
title = {{Cooks or Cobblers? Crowd Creativity through Combination}},
year = {2011}
}
@article{Lakhani2007,
abstract = {“No matter who you are, most of the smartest people work for someone else” is known as Joy's Law in the high-tech industry. Attributed to Sun Microsystems cofounder Bill Joy, this “law” emphasizes the essential knowledge problem that faces many enterprises today, that is, that in any given sphere of activity most of the pertinent knowledge will reside outside the boundaries of any one organization, and the central challenge for those charged with the innovation mission is to find ways to access that knowledge. In this article, we first provide an overview of distributed innovation systems that are achieving success in three different industries with three different organizational models.We then consider in the context of these three examples questions and concerns related to why people participate, the organizing principles of production, and the implications for intellectual property. We close our discussion with a review of potential extensions and limitations of this alternative model of innovation.},
author = {Lakhani, Karim R. and Panetta, Jill A.},
file = {:Users/vineetp/Library/Application Support/Mendeley Desktop/Downloaded/Lakhani, Panetta - 2007 - The Principles of Distributed Innovation(2).pdf:pdf},
isbn = {1558-2477},
issn = {1558-2477},
journal = {Innovations: Technology, Governance, Globalization},
number = {3},
pages = {97--112},
title = {{The Principles of Distributed Innovation}},
volume = {2},
year = {2007}
}
@article{Yu2012,
abstract = {Creativity is individual, and it is social. The social aspects of creativity have become of increasing interest as systems have emerged that mobilize large numbers of people to engage in creative tasks. We examine research related to collective intelligence and differentiate work on collective creativity from other collective activities by analyzing systems with respect to the tasks that are performed and the outputs that result. Three types of systems are discussed: games, contests and networks. We conclude by suggesting how systems that generate collective creativity can be improved and how new systems might be constructed.},
