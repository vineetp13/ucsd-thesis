@article{Liu2006,
abstract = {This paper focuses on peer feedback in relation to assessment processes. It examines the rationale for peer feedback, emphasising its potential for enhanced student learning. We draw on relevant literature to argue that the dominance of peer assessment processes using grades can undermine the potential of peer feedback for improving student learning. The paper throws further light on the issue by drawing on a large-scale questionnaire survey of tertiary students (1,740) and academics (460) in Hong Kong, supplemented by interview data. The findings indicate that a significant number of academics and students resist peer assessment using grades and that the majority report that students never or rarely grade each other in assessment activities. This paper explores why there is resistance, in particular, by academics to peer assessment and argues the case for a peer feedback process as an end in itself or as a precursor to peer assessment involving the awarding of marks. It also recommends some strategies for promoting peer feedback, through engaging students with criteria and for embedding peer involvement within normal course processes.},
archivePrefix = {arXiv},
arxivId = {arXiv:1011.1669v3},
author = {Liu, Ngar-Fun and Carless, David},
doi = {10.1017/CBO9781107415324.004},
eprint = {arXiv:1011.1669v3},
file = {:Users/vineetp/Library/Application Support/Mendeley Desktop/Downloaded/Liu, Carless - 2006 - Peer feedback The learning element of peer assessment.pdf:pdf},
isbn = {9788578110796},
issn = {1098-6596},
journal = {Teaching in Higher Education},
keywords = {icle},
number = {3},
pages = {279--290},
pmid = {25246403},
title = {{Peer feedback: The learning element of peer assessment}},
url = {http://hdl.handle.net/10722/54282},
volume = {11},
year = {2006}
}
@article{Yeager2013,
abstract = {Many foundational learning tasks are uninteresting to learners. This paper proposes that when it is difficult to make a learning task interesting it can be effective to make it meaningful by promoting a prosocial, beyond-the-self-oriented purpose for learning. When this is done, self- regulation may be increased. This general conclusion was supported in four studies with over 2,500 participants. Study 1 documented a correlation between a beyond-the-self purpose for learning and both trait and behavioral measures of academic self-regulation. On average this correlation was stronger than the relation between the same measures of self-regulation and more self-oriented, intrinsic interest-driven motives. Next, Study 2 showed that a brief, one-time psychological intervention promoting a beyond-the-self purpose for learning could improve overall high school GPA at the end of the semester. Studies 3 and 4 explored psychological mechanisms, showing that the beyond-the-self purpose could sustain self-regulatory behavior over the course of an increasingly-boring task (Study 3) and raise level of construal (Study 4), while an analogous experimental manipulation promoting more self-oriented, intrinsic-interest motives for learning did not exhibit these effects. Implications for theories of motivation, self- regulation, psychological abstraction, and educational intervention are discussed.},
author = {Yeager, David and Paunesku, Dave and D'Mello, Sidney and Spitzer, Brian and Duckworthh, Angela},
file = {:Users/vineetp/Library/Application Support/Mendeley Desktop/Downloaded/Yeager et al. - 2013 - It's Boring but it Matters A Beyond-the-Self Purpose for Learning Raises Construal Level and Promotes Academic Se.pdf:pdf},
pages = {1--76},
title = {{It's Boring but it Matters: A Beyond-the-Self Purpose for Learning Raises Construal Level and Promotes Academic Self-Regulation}},
url = {http://homepage.psy.utexas.edu/HomePage/Group/YeagerLAB/ADRG/Pdfs/Yeager et al purpose for learning 9-16-13.pdf},
year = {2013}
}
