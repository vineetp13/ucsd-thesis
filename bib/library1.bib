@article{Hui2015,
abstract = {I propose that designing online communities in a way that promotes help seeking will encourage novice entrepreneurial engineers to seek relevant knowledge, skills, and dispositions from experienced peers and mentors. Entrepreneurial engineers have started using a variety of online communities to seek resources, obtain knowledge, and build their networks. For instance, our previous work shows that crowdfunding platforms provide an online space where people can launch a campaign and solicit financial resources from one's extended online network. While much research has been performed on identifying expert qualifications, few have focused on the help seeker perspective and why they reach out to certain mentors. I am currently creating a model of online help seeking practices among novice entrepreneurial engineers taking into consideration how they identify what features are most important when considering willingness to contact a help giver. Findings will be used to inform the design of online entrepreneurial mentor networks.},
author = {Hui, Julie S. and Gerber, Elizabeth M.},
doi = {10.1145/2675133.2675188},
