keywords = {Cheating,Crowdsourcing,Honey pot,Honor code,Massive open online course (MOOC),Mechanical Turk,Warning},
number = {6},
pages = {1--23},
title = {{Deterring Cheating in Online Environments}},
url = {http://www.scopus.com/inward/record.url?eid=2-s2.0-84942770987{\&}partnerID=tZOtx3y1},
volume = {22},
