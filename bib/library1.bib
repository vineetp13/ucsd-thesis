@article{Miyake1986a,
abstract = {When people try to understand complex physical devices (e.g., a sewing machine), they proceed in an iterative fashion. They seem to reach several points at which they claim to "understand" the device. Each point of understanding is incomplete and requires a new level of understanding. As a result, they cycle between understanding and non-understanding as they traverse different levels. The present study provides a framework to capture the iterative nature of understanding. These points are discussed and illustrated through observations of three pairs of people constructively interacting to understand how a sewing machine works. In addition to the iterative search for understanding, the conceptual point in space from which the speaker appeared to be viewing the machine was important. This conceptual point of view (C-POV) was reflected in their use of language. The C-POV appeared to be stable during points of understanding and to shift frequently at points of non-understanding. ?? 1997 Elsevier Science Ltd. All rights reserved.},
author = {Miyake, Naomi},
doi = {10.1016/S0364-0213(86)80002-7},
file = {:Users/vineetp/Library/Application Support/Mendeley Desktop/Downloaded/Miyake - 1986 - Constructive interaction and the iterative process of understanding.pdf:pdf},
isbn = {0364-0213},
issn = {03640213},
journal = {Cognitive Science},
number = {2},
pages = {151--177},
title = {{Constructive interaction and the iterative process of understanding}},
volume = {10},
year = {1986}
}
