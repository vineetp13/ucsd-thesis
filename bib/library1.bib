issn = {1756-1833},
journal = {Bmj},
number = {jan28 9},
pages = {g368--g368},
pmid = {24472779},
title = {{Subjects no more: what happens when trial participants realize they hold the power?}},
url = {http://www.bmj.com/cgi/doi/10.1136/bmj.g368},
volume = {348},
year = {2014}
}
