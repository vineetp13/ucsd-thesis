journal = {ArXiV},
keywords = {Attributes,Computer Vision,Crowdsourcing,Dataset,Image,Knowledge,Language,Objects,Question Answering,Relationships,Scene Graph},
pages = {1--44},
title = {{Visual Genome}},
url = {https://arxiv.org/pdf/1602.07332.pdf},
year = {2016}
}
