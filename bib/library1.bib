journal = {Proc. of ACM symposium on Principles of Distributed Computing (PODC)},
keywords = {Nash equilibria,caching,distributed systems,game-theoretic models,peer-to-peer systems,price of anarchy},
pages = {21--30},
title = {{Selfish caching in distributed systems: a game-theoretic analysis}},
