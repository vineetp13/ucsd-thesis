keywords = {Academic Achievement,College Preparation,Communication Skills,Cooperation,Critical Thinking,Curriculum,Education Work Relationship,Ethics,Faculty Development,Informal Education,Learning Motivation,Learning Strategies,Museums,Persistence,Problem Solving,Public Education,Self Management,Skill Development,Student Evaluation,Teaching Methods,Thinking Skills,Transfer of Training},
pages = {242},
title = {{Education for Life and Work: Developing Transferable Knowledge and Skills in the 21st Century}},
url = {http://mhalabs.org/wp-content/uploads/2014/04/Pellegrino{\_}Report{\_}Education-for-Work-and-Life.pdf{\%}5Cnhttp://www.nap.edu/catalog/13398},
