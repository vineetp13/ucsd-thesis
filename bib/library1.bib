@article{Halverson2012,
abstract = {This paper explores the curious ways in which new technologies have, and have not, been$\backslash$ntaken up by schools. We begin with an exploration of how digital technologies have transformed$\backslash$neveryday learning and entertainment practices, and though classrooms have been relatively$\backslash$nunaffected by these new technologies; school management practices have been completely$\backslash$ntransformed by data technologies. We propose that these two levels of reform provide the basis$\backslash$nfor an illustrative contrast between technologies for education and technologies for learners. The$\backslash$ndifferences in adoption are not, we argue, a result of kinds of technology at play. Rather, we$\backslash$nsuggest that the patterns of adoption result from differing cultures of use: schools adapt$\backslash$ntechnologies in accountability cultures, and learners adapt technologies in participatory cultures.$\backslash$nOrganizational theorists have long observed how institutions adapt innovations to uses that are$\backslash$nconsistent with existing cultures and practices (Argyris {\&} Sch{\"{o}}n, 1996). The same kinds of$\backslash$ntechnologies can support a wide range of practices depending on how users frame the problems$\backslash$nthat need to be solved (Orlikowski, 2000). Our contrast between technologies for education and$\backslash$nfor learners is intended to highlight the how the cultures of use in schools and in the world$\backslash$nexplain the ways that technologies have, and have not, flourished in schools, and point toward$\backslash$nproductive opportunities for schools to embrace the power of new technologies.},
author = {Halverson, Rich and Shapiro, R. Ben},
file = {:Users/vineetp/Library/Application Support/Mendeley Desktop/Downloaded/Halverson, Shapiro - 2012 - Technologies for Education and Technologies for Learners How Information Technologies Are (and Should Be)(2).pdf:pdf},
journal = {in Press},
pages = {3--35},
title = {{Technologies for Education and Technologies for Learners: How Information Technologies Are (and Should Be) Changing Schools}},
year = {2012}
}
