journal = {Proceedings of the SIGCHI Conference on Human Factors in Computing Systems (CHI 2017)},
keywords = {Author's kit,Conference Publications,Guides,instructions},
title = {{Examining Menstrual Tracking to Inform the Design of Personal Informatics Tools}},
