@article{Coppola2015,
abstract = {As routine aspects of undergraduate instruction are becoming more automated and mechanized, high impact practices are moving to the center of the educational mission. High-level tasks and assignments can add critical learning-related attitudes, values, and beliefs, including disciplinary dispositions, to lessons in the subject matter. The sources, generation, and evaluation of new knowledge can all become part of the mainstream educational experience. Using an authentic learning framework, and a situated learning context, we provide a set of concrete instructional design strategies for chemistry instructors to think about adding Real Work (authentic learning activities), rather than homework, to their courses.},
