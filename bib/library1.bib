issn = {1849-4544},
journal = {Journal of Circulating Biomarkers},
keywords = {antibiotics,bacteria,cancer,gut,microbiome,personalized medicine,prenatal,short course,therapy},
pages = {1},
title = {{Short Course in the Microbiome}},
url = {http://www.intechopen.com/journals/journal-of-circulating-biomarkers/short-course-in-the-microbiome},
