@article{Kulkarni2014a,
abstract = {At a doctoral symposium in 2014 Kulkarni presented a conversation about "peer opportunities for learning at scale". Inspired by the powerful role peers play in traditional classrooms Kulkani proposes to leverage interactions amongst students. Extending the argument that peer interactions shift students from ‘automatic' thinking to more ‘active, effortful, conscious' thinking, which aids learning and growth [6]. Kulkarni claims 'that connecting students with the diverse experience levels and viewpoints of their global peer group peers improves learning outcomes'. Arguing 'for the co-development of pedagogical strategy and supporting software systems that are designed for scale and diversity' Kulkarni discusses how codevelopment of software and pedagogy can create 'novel educational experiences that not only leverage diversity and scale, but would be impossible without them'. The large scale of online classes and the diversity of the students that participate in them can enable new educational systems. This massive scale and diversity can enable always-available systems that help students share diverse ideas, and inspire and learn from each other. We introduce systems for two core educational processes at scale: discussion and assessment. To date, several thousand students in a dozen online classes have used our discussion system. Controlled experiments suggest that participants in more diverse discussions perform better on tests and that discussion improves engagement. Similarly, more than 100,000 students have reviewed peer work for both summative assessment and feedback. Through these systems, we argue that to create new educational experiences at scale, pedagogical strategies and software that leverage scale and diversity must be co-developed. More broadly, we suggest the key to creating new educational experiences online lies in leveraging massive networks of peers.},
author = {Kulkarni, Chinmay},
doi = {10.1145/2658779.2661169},
file = {:Users/vineetp/Library/Application Support/Mendeley Desktop/Downloaded/Kulkarni - 2014 - Making distance matter leveraging scale and diversity in massive online classes.pdf:pdf},
isbn = {9781450330688},
journal = {Proceedings of the adjunct publication of the 27th {\ldots}},
pages = {25--28},
title = {{Making distance matter: leveraging scale and diversity in massive online classes}},
url = {http://dl.acm.org/citation.cfm?id=2661169},
year = {2014}
}
