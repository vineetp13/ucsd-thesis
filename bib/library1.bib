file = {:Users/vineetp/Library/Application Support/Mendeley Desktop/Downloaded/Bagozzi, Davis, Warshaw - 1992 - Development and Test of a Theory of Technological Learning and Usage.pdf:pdf},
isbn = {00187267},
issn = {0018-7267},
keywords = {adoption of technology,attitudes,construct validity,trying},
pages = {659--686},
pmid = {4943428},
title = {{Development and Test of a Theory of Technological Learning and Usage}},
volume = {45},
year = {1992}
}
