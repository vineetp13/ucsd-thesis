@article{Consolvo2008,
abstract = {Recent advances in small inexpensive sensors, low-power processing, and$\backslash$nactivity modeling have enabled applications that use on-body sensing and$\backslash$nmachine learning to infer people's activities throughout everyday life.$\backslash$nTo address the growing rate of sedentary lifestyles, we have developed a$\backslash$nsystem, UbiFit Garden, which uses these technologies and a personal,$\backslash$nmobile display to encourage physical activity. We conducted a 3-week$\backslash$nfield trial in which 12 participants used the system and report findings$\backslash$nfocusing on their experiences,with the sensing and activity inference.$\backslash$nWe discuss key implications for systems that use on-body sensing and$\backslash$nactivity inference to encourage physical activity.},
