pages = {2579--2584},
title = {{Inequality in quality: Addressing socioeconomic, racial, and ethnic disparities in health care}},
url = {http://dx.doi.org/10.1001/jama.283.19.2579},
volume = {283},
year = {2000}
}
@article{Schwartz1998,
abstract = {In this article, the authors describe research about a method for developing prior knowledge that prepares students to learn from a text or lecture. They propose that analyzing contrasting cases can help learners generate the differentiated knowledge structures that enable them to understand a text deeply. In 3 classroom studies, college students analyzed contrasting cases that consisted of simplified experimental designs and data from classic psychology experiments. They then received a lecture or text on the psychological phenomena highlighted in the experiments. Approximately 1 wk later, they predicted outcomes for a hypothetical experiment that could be interpreted in light of the concepts they had studied. Generating the distinctions between contrasting cases and then reading a text or hearing a lecture led to more accurate predictions than the control treatments of (1) reading about the distinctions between the cases and hearing a lecture, (2) summarizing a relevant text and hearing a lecture, and (3) analyzing the contrasting cases twice without a lecture. Results indicate that there is a place for lectures and reading in the classroom if students have sufficiently differentiated domain knowledge to use the expository materials in a generative manner. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
address = {US},
