journal = {Proceedings of the International Joint Conference on Autonomous Agents and Multiagent Systems, AAMAS},
keywords = {Borda count,MOOCs,Peer grading,Social choice},
pages = {675--683},
title = {{Aggregating partial rankings with applications to peer grading in massive online open courses}},
url = {http://www.scopus.com/inward/record.url?eid=2-s2.0-84945188818{\&}partnerID=tZOtx3y1},
