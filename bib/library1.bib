@article{Kuznetsov2013a,
abstract = {When they, as members of the interaction design community, think about citizen science, they tend to envision people collecting, sharing, and acting on local data. Some of them might recall the famous success stories: the Christmas Bird Count, the longest-running citizen-driven bird census, or Project Budburst, which tracks environmental changes by monitoring plant lifecycles. Inspired by these and other examples, HCI research often discusses citizen science as non-experts acting as scientists. DIYbio (do-it-yourself biology) is a growing community of hobbyists, artists, hackers, and scientists experimenting with biology outside of professional laboratories. From independent bioartists, to meet-ups of hobbyists and professionals, biotech non-profits, and fully functional grassroots laboratories, the "garage biology" movement is reconfiguring and tinkering with organic materials and systems. Expanding the HCI community's view of citizen science to incorporate varying degrees of expertise could enable people to envision new types of sensing systems.},
author = {Kuznetsov, Stacey},
