journal = {Proceeding of the 17th international conference on World Wide Web - WWW '08},
keywords = {expertise finding,help seeking,knowledge sharing,online communities,question answering,social network anal-,ysis},
pages = {665--674},
title = {{Knowledge sharing and yahoo answers: everyone knows something}},
url = {http://portal.acm.org/citation.cfm?doid=1367497.1367587},
year = {2008}
}
@article{Engelbart1995,
abstract = {About this book: Boosting Our Collective IQ, a selection of readings, was originally published in a special edition of 2000 copies for attendees of the 4th International World Wide Web Conference, December 1995, of which 100 were hardbound, numbered and signed by the author, Douglas Engelbart, and by Tim Berners-Lee, the inventor of the World Wide Web. This book was the brainchild of Yuri Rubinski, co-founder of SoftQuad, who in addition to organizing Doug's 1995 SoftQuad Web Award at the conference, wished to create a special tribute for attendees that outlined Doug's vision for the future. So in collaboration with Doug's business partner and daughter Christina Engelbart (working under an oath of secrecy), Yuri produced this timeless keepsake to be distributed in a surprise unveiling to Doug at the award presentation. This new release has been published by MRW Connected for the Doug Engelbart Institute to mark the 40th anniversary of Doug's 1968 Demo. Our intent was to make this important work more widely available, while still honoring the original 2000 copies as limited edition keepsakes. This new edition is a near exact replica of the original softcover edition, in its artful style and craftsmanship, with the exception of the new cover design and title page.},
