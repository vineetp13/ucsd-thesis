pages = {158--162},
publisher = {Elsevier},
title = {{Successful collection of stool samples for microbiome analyses from a large community-based population of elderly men}},
url = {http://dx.doi.org/10.1016/j.conctc.2017.07.002},
volume = {7},
year = {2017}
