journal = {Proceedings of the National Academy of Sciences},
keywords = {Algorithms,Laboratories,Laboratories: organization {\&} administration,Nucleic Acid Conformation,RNA,RNA: chemistry,Software,User-Computer Interface},
number = {6},
pages = {2122--2127},
pmid = {24469816},
