@article{Arkadianos2007,
abstract = {BACKGROUND Gene-environment studies demonstrate variability in nutrient requirements depending upon individual variations in genes affecting nutrient metabolism and transport. This study investigated whether the inclusion of genetic information to personalize a patient's diet (nutrigenetics) could improve long term weight management. METHODS Patients with a history of failures at weight loss were offered a nutrigenetic test screening 24 variants in 19 genes involved in metabolism. 50 patients were in the nutrigenetic group and 43 patients attending the same clinic were selected for comparison using algorithms to match the characteristics: age, sex, frequency of clinical visits and BMI at initial clinic visit. The second group of 43 patients did not receive a nutrigenetic test. BMI reduction at 100 and {\textgreater} 300 days and blood fasting glucose were measured. RESULTS After 300 days of follow-up individuals in the nutrigenetic group were more likely to have maintained some weight loss (73{\%}) than those in the comparison group (32{\%}), resulting in an age and gender adjusted OR of 5.74 (95{\%} CI 1.74-22.52). Average BMI reduction in the nutrigenetic group was 1.93 kg/m2(5.6{\%} loss) vs. an average BMI gain of 0.51 kg/m2(2.2{\%} gain) (p {\textless} 0.023). Among patients with a starting blood fasting glucose of {\textgreater} 100 mg/dL, 57{\%} (17/30) of the nutrigenetic group but only 25{\%} (4/16) of the non-tested group had levels reduced to {\textless} 100 mg/dL after {\textgreater} 90 days of weight management therapy (OR for lowering glucose to {\textless} 100 mg/dL due to diet = 1.98 95{\%}CI 1.01, 3.87, p {\textless} 0.046). CONCLUSION Addition of nutrigenetically tailored diets resulted in better compliance, longer-term BMI reduction and improvements in blood glucose levels.},
author = {Arkadianos, Ioannis and Valdes, Ana M and Marinos, Efstathios and Florou, Anna and Gill, Rosalynn D and Grimaldi, Keith A},
doi = {10.1186/1475-2891-6-29},
file = {:Users/vineetp/Library/Application Support/Mendeley Desktop/Downloaded/Arkadianos et al. - 2007 - Improved weight management using genetic information to personalize a calorie controlled diet.pdf:pdf},
isbn = {1475289162},
issn = {1475-2891},
journal = {Nutrition Journal},
number = {1},
pages = {29},
pmid = {17945020},
title = {{Improved weight management using genetic information to personalize a calorie controlled diet}},
url = {http://nutritionj.biomedcentral.com/articles/10.1186/1475-2891-6-29},
